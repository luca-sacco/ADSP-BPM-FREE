{
  "name": "adsp-bpm",
  "version": "2.51.3",
  "scripts": {
    "ng": "ng",
    "release": "node ./CI",
    "mocks": "node ./mocks/server.js",
    "start": "npm run serve:single-spa",
    "build": "npm run build:single-spa",
    "lint": "ng lint",
    "build:single-spa": "ng build --deploy-url /single-spa/adsp-bpm/ --output-hashing none",
    "serve:single-spa": "ng serve --disable-host-check --port 4900 --deploy-url http://localhost:4900/ --live-reload false",
    "conventional-changelog": "conventional-changelog",
    "prepare-commit-msg": "prepare-commit-msg",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "release-distribute": "node ./scripts",
    "build:staging": "ng build --deploy-url /single-spa/adsp-bpm/${version}/ --output-hashing none --prod"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^9.2.0",
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "sftp-upload": "^1.0.4",
    "single-spa": "^5.9.2",
    "single-spa-angular": "~4.3.1",
    "ssh2-sftp-client": "^8.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@release-it/conventional-changelog": "^4.3.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "codelyzer": "^5.1.2",
    "cors": "^2.8.4",
    "current-git-branch": "^1.1.0",
    "cz-conventional-changelog": "3.1.0",
    "dotenv-cli": "^3.2.0",
    "express": "^4.16.3",
    "glob": "^7.1.3",
    "husky": "^4.2.5",
    "inquirer": "^8.2.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "morgan": "^1.9.1",
    "protractor": "~7.0.0",
    "release-it": "^14.14.3",
    "shelljs": "^0.8.5",
    "ts-node": "3.2.2",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
